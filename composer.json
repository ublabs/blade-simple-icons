{
    "name": "ublabs/blade-simple-icons",
    "description": "A package to easily make use of Simple Icons in your Laravel Blade views.",
    "keywords": [
        "ublabs",
        "laravel",
        "blade-simple-icons"
    ],
    "homepage": "https://github.com/ublabs/blade-simple-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "Adri√°n UB",
            "homepage": "https://github.com/adrian-ub"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "blade-ui-kit/blade-icons": "^1.1.1",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "ext-dom": "*",
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Ublabs\\BladeSimpleIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ublabs\\BladeSimpleIcons\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ublabs\\BladeSimpleIcons\\BladeSimpleIconsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
